import plotly.plotly as py
from plotly.graph_objs import *
import pandas as pd
import numpy as np
import plotly.plotly as py
import plotly.graph_objs as go
import sys

print 'Argument List:', str(sys.argv)
fileName = sys.argv[1];
print fileName
df = pd.read_csv(fileName)
df.head()

trace1 = go.Scatter(
                    x=df['_Frame'], y=df['EyePos/x'], # Data
                    mode='lines', name='EyeXVariation' # Additional options
                   )
trace2 = go.Scatter(x=df['_Frame'], y=df['EyePos/y'], mode='lines', name='EyeYVariation' )

layout = go.Layout(title='Eye position Analysis',
                   plot_bgcolor='rgb(230, 230,230)')

fig = go.Figure(data=[trace1, trace2], layout=layout)

# Plot data in the notebook
py.iplot(fig, filename='commandLine')
